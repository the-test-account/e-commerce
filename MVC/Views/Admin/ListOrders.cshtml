@model IEnumerable<Entities.OrderModel>
        
    <table class="table orderlist " >
        <thead>
    <tr>
        <th>

                    Adress
        </th>
        <th>
                    Kontakt uppgift
        </th>
        <th>
                    Order datum
        </th>
        <th>
                    Order nummer
        </th>
        <th>
                    Totallt summa
        </th>
        <th>
                    Leveranssätt
        </th>
        <th>
                    Betalningssätt
        </th>
                <th width="200">
                    Kommentar
                </th>
        <th>
                    Ändringar
        </th>
    </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
    <tr>
        <td>

                        @Html.DisplayFor(modelItem => item.Address.Street)
                        <br />
                        @Html.DisplayFor(modelItem => item.Address.City)
                        <br />
                        @Html.DisplayFor(modelItem => item.Address.ZipCode)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.Contact.FirstName)
                        @Html.DisplayFor(modelItem => item.Contact.LastName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.DeliveryType.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PaymentType.Name)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.ActionLink("Detaljer", "Details", new { id=item.Id }) |
            @Html.ActionLink("Radera", "DeleteOrder", new { id=item.Id })
        </td>
    </tr>
}
        </tbody>
</table>
